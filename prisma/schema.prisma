generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id        String   @id @default(cuid())
  userName  String
  category  String
  sentiment String
  comment   String
  gender    String
  age       Int
  rating    Int
  createdAt DateTime @default(now())
}
